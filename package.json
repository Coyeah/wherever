{
  "name": "wherever",
  "version": "0.5.0",
  "description": "Tiny nodejs web server",
  "bin": {
    "wherever": "bin/index.js"
  },
  "main": "lib/index",
  "files": [
    "lib/**/*",
    "bin/**/*"
  ],
  "scripts": {
    "clean": "rimraf lib",
    "tpl": "node_modules/.bin/cpr src/template lib",
    "build": "npm run clean && node_modules/.bin/rollup --config rollup.config.js && npm run tpl"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Coyeah/wherever.git"
  },
  "keywords": [
    "tiny",
    "static",
    "nodejs",
    "web_server"
  ],
  "author": "coyeah <Coyeah_chen@outlook.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Coyeah/wherever/issues"
  },
  "homepage": "https://github.com/Coyeah/wherever#readme",
  "dependencies": {
    "chalk": "^4.1.1",
    "ejs": "^3.1.6",
    "express": "^4.17.1",
    "http-proxy-middleware": "^1.3.1",
    "mime-types": "^2.1.30",
    "yargs": "^17.0.1"
  },
  "devDependencies": {
    "cpr": "^3.0.1",
    "rimraf": "^3.0.2",
    "rollup": "^2.47.0",
    "rollup-plugin-terser": "^7.0.2"
  }
}
